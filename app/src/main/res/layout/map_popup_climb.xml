<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ToggleButton
        android:id="@+id/c2a"
        android:layout_width="80dp"
        android:layout_height="75dp"
        android:enabled="false"
        android:onClick="onClickClimbBtn"
        android:textOff="NO"
        android:textOn="CLIMB"
        app:layout_constraintStart_toEndOf="@+id/c1a"
        app:layout_constraintTop_toTopOf="@+id/c1a" />

    <ToggleButton
        android:id="@+id/c1a"
        android:layout_width="80dp"
        android:layout_height="75dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:onClick="onClickClimbBtn"
        android:text="ToggleButton"
        android:textOff="NO"
        android:textOn="CLIMB"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pa" />

    <ToggleButton
        android:id="@+id/c3a"
        android:layout_width="80dp"
        android:layout_height="75dp"
        android:enabled="false"
        android:onClick="onClickClimbBtn"
        android:text="ToggleButton"
        android:textOff="NO"
        android:textOn="CLIMB"
        app:layout_constraintStart_toEndOf="@+id/c2a"
        app:layout_constraintTop_toTopOf="@+id/c2a" />

    <ToggleButton
        android:id="@+id/c3c"
        android:layout_width="80dp"
        android:layout_height="75dp"
        android:enabled="false"
        android:onClick="onClickClimbBtn"
        android:text="ToggleButton"
        android:textOff="NO"
        android:textOn="CLIMB"
        app:layout_constraintStart_toEndOf="@+id/c2c"
        app:layout_constraintTop_toTopOf="@+id/c2c" />

    <ToggleButton
        android:id="@+id/c1c"
        android:layout_width="80dp"
        android:layout_height="75dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="4dp"
        android:enabled="false"
        android:onClick="onClickClimbBtn"
        android:text="ToggleButton"
        android:textOff="NO"
        android:textOn="CLIMB"
        app:layout_constraintStart_toEndOf="@+id/c3a"
        app:layout_constraintTop_toBottomOf="@+id/pc" />

    <ToggleButton
        android:id="@+id/c2c"
        android:layout_width="80dp"
        android:layout_height="75dp"
        android:enabled="false"
        android:onClick="onClickClimbBtn"
        android:text="ToggleButton"
        android:textOff="NO"
        android:textOn="CLIMB"
        app:layout_constraintStart_toEndOf="@+id/c1c"
        app:layout_constraintTop_toTopOf="@+id/c1c" />

    <ToggleButton
        android:id="@+id/level"
        android:layout_width="454dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:enabled="false"
        android:textOff="LEVEL"
        android:textOn="LEVEL"
        app:layout_constraintEnd_toEndOf="@+id/c3c"
        app:layout_constraintStart_toStartOf="@+id/c1a"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Atempted"
        app:layout_constraintEnd_toEndOf="@+id/c3a"
        app:layout_constraintStart_toStartOf="@+id/c1a"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Climb"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/c3c"
        app:layout_constraintStart_toStartOf="@+id/c1a"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Actual"
        app:layout_constraintEnd_toEndOf="@+id/c3c"
        app:layout_constraintStart_toStartOf="@+id/c1c"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Self"
        app:layout_constraintEnd_toStartOf="@+id/c2a"
        app:layout_constraintStart_toStartOf="@+id/c1a"
        app:layout_constraintTop_toBottomOf="@+id/c1a" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Other Bots"
        app:layout_constraintEnd_toEndOf="@+id/c3a"
        app:layout_constraintStart_toStartOf="@+id/c2a"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Self"
        app:layout_constraintEnd_toEndOf="@+id/c1c"
        app:layout_constraintStart_toStartOf="@+id/c1c"
        app:layout_constraintTop_toBottomOf="@+id/c1c" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Other Bots"
        app:layout_constraintEnd_toEndOf="@+id/c3c"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toEndOf="@+id/c1c"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <Space
        android:id="@+id/space2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toEndOf="@+id/c3c"
        app:layout_constraintTop_toBottomOf="@+id/button7" />

    <Button
        android:id="@+id/button6"
        android:layout_width="225dp"
        android:layout_height="50dp"
        android:layout_marginTop="24dp"
        android:onClick="onClickCancelClimb"
        android:text="CANCEL"
        app:layout_constraintStart_toStartOf="@+id/level"
        app:layout_constraintTop_toBottomOf="@+id/level" />

    <Button
        android:id="@+id/button7"
        android:layout_width="225dp"
        android:layout_height="50dp"
        android:onClick="onClickDoneClimb"
        android:text="DONE"
        app:layout_constraintEnd_toEndOf="@+id/level"
        app:layout_constraintTop_toTopOf="@+id/button6" />

    <ToggleButton
        android:id="@+id/pa"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:onClick="onClickClimbBtn"
        android:textOff="PARK"
        android:textOn="PARK"
        app:layout_constraintEnd_toEndOf="@+id/c3a"
        app:layout_constraintStart_toStartOf="@+id/c1a"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ToggleButton
        android:id="@+id/pc"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:enabled="false"
        android:onClick="onClickClimbBtn"
        android:text="ToggleButton"
        android:textOff="PARK"
        android:textOn="PARK"
        app:layout_constraintEnd_toEndOf="@+id/c3c"
        app:layout_constraintStart_toStartOf="@+id/c1c"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

</androidx.constraintlayout.widget.ConstraintLayout>